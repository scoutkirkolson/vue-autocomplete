{
  "name": "sko-autocomplete",
  "version": "0.0.10",
  "description": "Vue2 Autocomplete component",
  "author": "Kirk Olson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KirkOlson/vue-autocomplete/issues"
  },
  "homepage": "https://github.com/KirkOlson/vue-autocomplete#readme",
  "main": "dist/sko-autocomplete.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "devpoi": "poi build --format cjs --dist dist; poi build --format umd --dist dist/umd",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "watch-poll": "yarn watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development webpack --hot --progress --hide-modules ",
    "prod": "yarn production",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KirkOlson/vue-autocomplete.git"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "devDependencies": {
    "cross-env": "^5.0.5",
    "file-loader": "^1.1.11",
    "node-sass": "^4.8.3",
    "poi": "^9.5.2",
    "sass-loader": "^7.0.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.5.5",
    "webpack-cli": "^1.5.3"
  },
  "dependencies": {
    "es6lib": "git+ssh://git@github.com/scoutkirkolson/es6lib.git",
    "jquery": "^3.3.1",
    "moment": "^2.22.0"
  },
  "files": [
    "dist/sko-autocomplete.js",
    "dist/sko-autocomplete.js.map"
  ]
}
